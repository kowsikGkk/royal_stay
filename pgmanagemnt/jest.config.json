{
  "moduleFileExtensions": [
    "js",
    "jsx"
  ],
  "moduleDirectories": [
    "node_modules"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "identity-obj-proxy"
  },
  "collectCoverageFrom": [
    "src/tests/**/*.test.js",
    "!**/node_modules/**"
  ],
  "coveragePathIgnorePatterns": [
    "/node_modules/"
  ],
  "setupFiles": [
    "./setupFile.js"
  ],
  "testEnvironment": "node",
  "transform": {
    "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "./fileTransformer.js",
    "^.+\\.js$": "babel-jest"
  }
}