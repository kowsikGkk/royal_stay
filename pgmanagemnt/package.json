    "scripts": {
        "test": "jest --config jest.config.json --updateSnapshot",
        "start": "npm i && concurrently --kill-others \"better-npm-run start-default\"",
        "dev": "npm i && concurrently --kill-others \"better-npm-run start-dev\"",
        "staging": "npm i && concurrently --kill-others \"better-npm-run start-staging\"",
        "clean": "rimraf ./__build__",
        "build": "npm i && npm run clean && NODE_ENV=production webpack --display-error-details --config webpack/webpack.prod.config.js --progress -p",
        "stylelint": "stylelint ./src/**/*.css --fix",
        "eslint": "eslint src --fix",
        "lint": "npm run eslint | npm run stylelint"
    },
    "repository": {
        "type": "git",
        "url": ""
    },
    "betterScripts": {
        "start-default": {
            "description": "This script will run the dev server with default ports",
            "command": "node server.js",
            "env": {
                "NODE_ENV": "local",
                "PORT": 3000,
                "API_HOST": "localhost",
                "API_PORT": 8090,
                "MODULE_NAME": "Default local Server",
                "FRONTEND": "http://localhost/"
            }
        },
        "start-dev": {
            "description": "This script will run the dev server with default ports",
            "command": "node server.js",
            "env": {
                "NODE_ENV": "development",
                "PORT": 3000,
                "API_HOST": "192.168.39.42",
                "API_PORT": 8090,
                "MODULE_NAME": "Test gcp Server",
                "FRONTEND": "http://35.227.63.85"
            }
        },
        "start-qa": {
            "description": "This script will run the qa server with default ports",
            "command": "node server.js",
            "env": {
                "NODE_ENV": "testing",
                "PORT": 3000,
                "API_HOST": "192.168.39.43",
                "API_PORT": 8090,
                "MODULE_NAME": "Test gcp Server",
                "FRONTEND": "http://192.168.39.43"
            }
        },
        "start-staging": {
            "description": "This script will run the staging server with default ports",
            "command": "node server.js",
            "env": {
                "NODE_ENV": "staging",
                "PORT": 8080,
                "API_HOST": "https://razor-backend.shopkick.com",
                "MODULE_NAME": "Default Staging Server",
                "FRONTEND": "https://razor-frontend.shopkick.com/"
            }
        }
    },
